.goodsInfo:last-child{
  margin-bottom:20rpx;
}
/* 商品详情款式详情 */
.style_compo{
  width:100%;
  background: #fff;  
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  padding:20rpx;
  height:164rpx;
}
.style_compo:last-child{
  border:none;
}
.style_compo>image{
  width:100rpx;
  height:100rpx;
  margin-right:20rpx
}
.style_compo .style_info{
  flex:3;
  height:100%;
  width:100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  align-content: stretch;
}
.style_info .style_detail{
  width:100%;
}

