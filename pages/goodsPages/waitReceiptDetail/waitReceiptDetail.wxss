.content{
  height: 100%;
  overflow-x:hidden ;
  overflow-y:auto; 
  flex: 3;
}
/* 物流公司信息 */
.logistic_title{
  margin:40rpx 0;
  font-size:28rpx;
  color: #707070;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding:0 20rpx;
}
.logistic_title image{
  width:44rpx;
  height:38rpx;
  margin-right:20rpx;
}

.logistic_data .data_title{
  padding:30rpx 20rpx 20rpx; 
}
.logisitc_list{
  width: 100%;
  padding:0 20rpx 20rpx 78rpx;
}
.logisitc_list_cell{
  width:100%;
  min-height:114rpx;
  position: relative;
}
.logisitc_list_cell:after{
  content: "";
  border-left:1px dotted #e2e2e2;
  height:70rpx;
  width:1px;
  position: absolute;
  top:40rpx;
  left:16rpx;
  z-index: 100;
}
.cell_inner>text{
  flex: 3;
}
.logisitc_list_cell:last-child{
  min-height:none;
}
.logisitc_list_cell:first-child .sel_icon{
  border:none;

}
.logisitc_list_cell .cell_inner{
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}
.cell_inner .sel_icon{
  height:34rpx;
  width:34rpx;
  margin-right:20rpx;
  border:1px solid #e2e2e2;
  border-radius: 50%;
}
.cell_inner .sel_icon image{
  width:100%;
  height: 100%;
}

.sec_title{
  font-size:16px;
  color:#333333;
  margin:40rpx 0 20rpx;
  padding:0 20rpx;
}

.goodsInfo{
  margin-bottom:20rpx;
}


/* 底部提示 */
.foot_tips{
  height:60rpx;
  line-height: 60rpx;
  margin-top:60rpx;
}